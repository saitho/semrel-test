{
  "name": "@saithodev/semantic-release-backmerge-example",
  "private": true,
  "main": "index.js",
  "release": {
    "ci": false,
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@saithodev/semantic-release-backmerge",
        {
          "message": "Testmessage",
          "backmergeBranches": [
            "develop",
            {
              "from": "main",
              "to": "another-branch"
            }
          ]
        }
      ]
    ]
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saitho/semrel-test.git"
  },
  "dependencies": {
    "@saithodev/semantic-release-backmerge": "github:evenius/semantic-release-backmerge#semantic_esm_support",
    "semantic-release": "^22.0.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
